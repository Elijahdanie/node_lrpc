{
    "compilerOptions": {
      "target": "es2017",
      "module": "commonjs",
      "moduleResolution": "node",
      "strict": true,                  
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "sourceMap": false,
      "skipLibCheck": true,
      "strictNullChecks": false,
      "pretty": true,
      "noImplicitAny": false,
      "resolveJsonModule": true,
    },
    "exclude": [
    ]
  }
